prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=@CMAKE_INSTALL_FULL_LIBDIR@
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@

Name: QArv
Description: Qt GUI and API interfaces to ethernet cameras via Aravis
Version: @qarv_VERSION@
Requires:
Libs: -L${libdir} -lqarv-@qarv_API@
Cflags: -I${includedir}/qarv-@qarv_API@
